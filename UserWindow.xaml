<Window x:Class="ProjectFilm.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectFilm"
        mc:Ignorable="d"
        Title="UserWindow" Height="450" Width="600">
    <Grid>
        <Button x:Name="UserProfileButton"  BorderBrush="{x:Null}"  HorizontalAlignment="Right" Margin="0,9,10,0" VerticalAlignment="Top" RenderTransformOrigin="-0.426,0.451" FontSize="8" Click="UserProfileButton_Click" Background="White" Width="35">
            <Button.Content>
                <Image Source="Image\user_profile_icon_208590.png" Stretch="Fill" OpacityMask="Black" RenderTransformOrigin="1.216,0.49" Height="30"  />
            </Button.Content>
        </Button>
        <Button x:Name ="BackButton"  BorderBrush="{x:Null}"  HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="BackButton_Click" Background="White">
            <Button.Content>
                <Image Source="Image\BackArrow.png" Stretch="Fill" Height="29" Width="32" OpacityMask="Black" />
            </Button.Content>
        </Button>

    </Grid>
</Window>